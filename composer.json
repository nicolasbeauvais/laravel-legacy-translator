{
    "name": "nicolasbeauvais/laravel-legacy-translator",
    "description": "A laravel package to translate a legacy app in no time",
    "keywords": [
        "translator",
        "translation",
        "legacy",
        "translated",
        "i18n",
        "localisation",
        "laravel"
    ],
    "homepage": "https://github.com/nicolasbeauvais/laravel-legacy-translator",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Nicolas Beauvais",
            "email": "nicolasbeauvais1@gmail.com",
            "homepage": "http://nicolas-beauvais.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/console": "~5.1.16|~5.2.0|~5.3.0",
        "illuminate/support": "~5.1.16|~5.2.0|~5.3.0",
        "imangazaliev/didom": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "NicolasBeauvais\\LegacyTranslator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NicolasBeauvais\\LegacyTranslator\\Test\\": "tests"
        }
    }
}
